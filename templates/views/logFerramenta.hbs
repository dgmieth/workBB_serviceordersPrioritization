<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
<!-- HANDLEBARS -->
    <title>{{title}}</title>
<!-- BOOTSTRAP CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
<!-- CUSTOMIZED CSS -->
    <link rel="stylesheet" href="/css/header.css">
    <link rel="stylesheet" href="/css/footer.css">
    <link rel="stylesheet" href="/css/mainPage.css">
<!-- DATATABLES CSS -->
    <link rel="stylesheet" href="/css/datatables/dataTables.min.css">
    <link rel="stylesheet" href="/css/datatables/select.dataTables.min.css">
    <link rel="stylesheet" href="/css/datatables/jquery.dataTables.min.css">
    <link rel="stylesheet" href="/css/datatables/fixedHeader.dataTables.css">
    <link rel="stylesheet" href="/css/datatables/dataTables.css">
<!-- ICON -->
    <link rel="icon" type="image/ico" href="/img/bbFavicon.ico">
</head>
<body>
    <header>{{> header}}</header>
<!-- ================================================================================================= -->
<div id="spinner" class="container-fluid mx-auto my-auto text-center" style="display: block;">
    <img src="/img/spinner200.gif" alt="">
</div>
<!--INICIO - DATATABLE -->
    <div id="table-priorizados-todos-wrapper" class=".container-fluid" style="margin-bottom: 0px;display:none">
        <div class=".container-fluid" style="margin-bottom:100px;padding-left:35px;padding-right:35px">
            <div class="row">
                <div class="col-lg-12">
                    <table id="table-log" class="table table-striped table-hover" cellspacing="0" style="width:100%;color: black;">
                        <thead>
                            <tr>
                                <th class="d-none" style="width: 190px;">id log</th>
                                <th class="d-table-cell d-sm-table-cell d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 190px;">Usuário</th>
                                <th class="d-none d-sm-none d-md-none d-lg-table-cell d-xl-table-cell" style="width: 190px;">Ação</th>
                                <th class="d-table-cell d-sm-table-cell d-md-table-cell d-lg-table-cell d-xl-table-cell" style="width: 190px;">Data</th>
                            </tr>
                        </thead>             
                    </table>
                </div>
            </div>
        </div>
    </div>
<!--FIM - DATATABLE -->
<!-- ================================================================================================= -->
    <footer class="">{{>footer}}</footer>
</body>
<!-- BOOTSTRAP JS -->
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
<script src="/js/bootstrapFramework/bootstrap.bundle.min.js"></script>
<!-- AJAX MOMENT JS -->
<script src="/js/datatables/ajax.moment.min.js"></script>
<!-- DATATABLE JS -->
<script src="/js/datatables/jquery.dataTables.min.js"></script>
<script src="/js/datatables/dataTables.fixedColumns.min.js"></script>
<script src="/js/datatables/dataTables.select.min.js"></script>
<script src="/js/datatables/dataTables.fixedHeader.js"></script>
<!-- CUSTOMIZED JS -->
<script src="/js/header.js"></script>
<script src="/js/logFerramenta/variablesAndFunctions.js"></script>
<script src="/js/logFerramenta/loadingDataTable.js"></script>
<script>
    $('#table-log thead tr').clone(true).appendTo( '#table-log thead' );
    $('#table-log thead tr:eq(1) th').each( function (i) {
        var title = $(this).text();
        if($(this).attr('class') === 'select-checkbox'){
        }else{
            $(this).html( '<input type="text" placeholder="'+title+'"/>' );
        }
            
        $( 'input', this ).on( 'keyup change', function () {
            if ( table.column(i).search() !== this.value ) {
                table
                    .column(i)
                    .search( this.value )
                    .draw();
            }
        } );
    } );
</script>
</html>
